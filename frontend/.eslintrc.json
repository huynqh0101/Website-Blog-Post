{
  "extends": [],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    // Disable the exhaustive-deps rule
    "react-hooks/exhaustive-deps": "off",
    "typescript-eslint/no-empty-object-type": "off",

    // Add your other rules here
    "no-unused-vars": "warn",
    "semi": ["error", "always"]
    // Other rules can be added as needed
  }
}
